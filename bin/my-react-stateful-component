#!/usr/bin/env node

const [, , ...args] = process.argv;

if (args.length !== 1) {
  console.error('Give me exactly one component name');
  process.exit(1);
}

const name = args[0];
console.log(`import React, { Component } from 'react';

class ${name} extends Component {
  render() {
    return (
      
    );
  }
}

export default ${name};`);
